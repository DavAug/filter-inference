<?xml version="1.0" encoding="UTF-8"?>
<sbml xmlns="http://www.sbml.org/sbml/level3/version2/core" level="3" version="2">
  <model id="chaves_tnf_network_model" timeUnits="second">

  # REMOVE UNNECESSARY PARAMETERS!!

    <listOfUnitDefinitions>
      <unitDefinition id="per_second">
        <listOfUnits>
          <unit kind="second" exponent="-1" scale="0" multiplier="1"/>
        </listOfUnits>
      </unitDefinition>
      <unitDefinition id="mg">
        <listOfUnits>
          <unit kind="gram" exponent="1" scale="-3" multiplier="1"/>
        </listOfUnits>
      </unitDefinition>
      <unitDefinition id="mg_per_L_per_second">
        <listOfUnits>
          <unit kind="gram" exponent="1" scale="-3" multiplier="1"/>
          <unit kind="liter" exponent="-1" scale="0" multiplier="1"/>
          <unit kind="second" exponent="-1" scale="0" multiplier="1"/>
        </listOfUnits>
      </unitDefinition>
      <unitDefinition id="L_per_mg_per_second">
        <listOfUnits>
          <unit kind="liter" exponent="1" scale="0" multiplier="1"/>
          <unit kind="gram" exponent="-1" scale="-3" multiplier="1"/>
          <unit kind="second" exponent="-1" scale="0" multiplier="1"/>
        </listOfUnits>
      </unitDefinition>
      <unitDefinition id="mg_per_L">
        <listOfUnits>
          <unit kind="gram" exponent="1" scale="-3" multiplier="1"/>
          <unit kind="liter" exponent="-1" scale="0" multiplier="1"/>
        </listOfUnits>
      </unitDefinition>
    </listOfUnitDefinitions>

    <listOfCompartments>
      <compartment id="central" name="central" size="1" units="liter"/>
    </listOfCompartments>

    <listOfSpecies>
      <!-- Tumour necrosis factor -->
      <species id="tumour_necrosis_factor" name="tumour_necrosis_factor" compartment="central" initialAmount="0" hasSubstanceUnits="false" substanceUnits="mg"/>
    </listOfSpecies>

    <listOfParameters>
      <!-- Activated caspase 3 -->
      <parameter id="caspase_3" value="1" constant="false" units="mg_per_L"/>

      <!-- Activated caspase 8 -->
      <parameter id="caspase_8" value="1" constant="false" units="mg_per_L"/>

      <!-- Nuclear transcription factor kappa B -->
      <parameter id="nf_kappaB" value="1" constant="false" units="mg_per_L"/>

      <!-- Inhibitor of nuclear transcription factor kappa B -->
      <parameter id="nf_kappaB_inhibitor" value="1" constant="false" units="mg_per_L"/>

      <!-- Caspase 8 activation rate, deactivation rate and modifier EC50s (assumes infinite supply of procaspase 8) -->
      <parameter id="half_maximal_inhibitory_concentration_c8_nf" value="1" constant="true" units="mg_per_L"/>
      <parameter id="half_maximal_effect_concentration_c8_tnf" value="1" constant="true" units="mg_per_L"/>
      <parameter id="half_maximal_effect_concentration_c8_c3" value="1" constant="true" units="mg_per_L"/>

      <!-- Caspase 3 activation rate, deactivation rate and modifier EC50s (assumes infinite supply of procaspase 3) -->
      <parameter id="half_maximal_inhibitory_concentration_c3_nf" value="1" constant="true" units="mg_per_L"/>
      <parameter id="half_maximal_effect_concentration_c3_c8" value="1" constant="true" units="mg_per_L"/>

      <!-- Nuclear transcription factor kappa B activation rate, deactivation rate and modifier EC50s -->
      <parameter id="half_maximal_inhibitory_concentration_nf_inf" value="1" constant="true" units="mg_per_L"/>
      <parameter id="half_maximal_inhibitory_concentration_nf_c3" value="1" constant="true" units="mg_per_L"/>

      <!-- Nuclear transcription factor inhibitor activation rate, deactivation rate and modifier EC50s -->
      <parameter id="half_maximal_inhibitory_concentration_inf_tnf" value="1" constant="true" units="mg_per_L"/>
      <parameter id="half_maximal_inhibitory_concentration_inf_nf" value="1" constant="true" units="mg_per_L"/>

      <!-- Tumour necrosis factor elimination rate -->
      <parameter id="elimination_rate" value="1" constant="true" units="per_second"/>
    </listOfParameters>

    <listOfRules>
      <!-- Caspase 3 -->
      <rateRule variable="caspase_3">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <minus/>
            <apply>
              <times/>
              <cn> 1 </cn>
              <apply>
                <divide/>
                <apply>
                  <power/>
                  <ci> half_maximal_inhibitory_concentration_c3_nf </ci>
                  <cn> 2 </cn>
                </apply>
                <apply>
                  <plus/>
                  <apply>
                    <power/>
                    <ci> nf_kappaB </ci>
                    <cn> 2 </cn>
                  </apply>
                  <apply>
                    <power/>
                    <ci> half_maximal_inhibitory_concentration_c3_nf </ci>
                    <cn> 2 </cn>
                  </apply>
                </apply>
              </apply>
              <apply>
                <divide/>
                <apply>
                  <power/>
                  <ci> caspase_8 </ci>
                  <cn> 2 </cn>
                </apply>
                <apply>
                  <plus/>
                  <apply>
                    <power/>
                    <ci> caspase_8 </ci>
                    <cn> 2 </cn>
                  </apply>
                  <apply>
                    <power/>
                    <ci> half_maximal_effect_concentration_c3_c8 </ci>
                    <cn> 2 </cn>
                  </apply>
                </apply>
              </apply>
            </apply>
            <apply>
              <times/>
              <cn> 1 </cn>
              <ci> caspase_3 </ci>
            </apply>
          </apply>
        </math>
      </rateRule>

      <!-- Caspase 8 -->
      <rateRule variable="caspase_8">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <minus/>
            <apply>
              <plus/>
              <apply>
                <times/>
                <cn> 0.5 </cn>
                <apply>
                  <divide/>
                  <apply>
                    <power/>
                    <ci> half_maximal_inhibitory_concentration_c8_nf </ci>
                    <cn> 2 </cn>
                  </apply>
                  <apply>
                    <plus/>
                    <apply>
                      <power/>
                      <ci> nf_kappaB </ci>
                      <cn> 2 </cn>
                    </apply>
                    <apply>
                      <power/>
                      <ci> half_maximal_inhibitory_concentration_c8_nf </ci>
                      <cn> 2 </cn>
                    </apply>
                  </apply>
                </apply>
                <apply>
                  <divide/>
                  <apply>
                    <power/>
                    <ci> tumour_necrosis_factor </ci>
                    <cn> 2 </cn>
                  </apply>
                  <apply>
                    <plus/>
                    <apply>
                      <power/>
                      <ci> tumour_necrosis_factor </ci>
                      <cn> 2 </cn>
                    </apply>
                    <apply>
                      <power/>
                      <ci> half_maximal_effect_concentration_c8_tnf </ci>
                      <cn> 2 </cn>
                    </apply>
                  </apply>
                </apply>
              </apply>
              <apply>
                <times/>
                <cn> 0.5 </cn>
                <apply>
                  <divide/>
                  <apply>
                    <power/>
                    <ci> caspase_3 </ci>
                    <cn> 2 </cn>
                  </apply>
                  <apply>
                    <plus/>
                    <apply>
                      <power/>
                      <ci> caspase_3 </ci>
                      <cn> 2 </cn>
                    </apply>
                    <apply>
                      <power/>
                      <ci> half_maximal_effect_concentration_c8_c3 </ci>
                      <cn> 2 </cn>
                    </apply>
                  </apply>
                </apply>
              </apply>
            </apply>
            <apply>
              <times/>
              <cn> 1 </cn>
              <ci> caspase_8 </ci>
            </apply>
          </apply>
        </math>
      </rateRule>

      <!-- Nuclear transcription factor kappa B -->
      <rateRule variable="nf_kappaB">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <minus/>
            <apply>
              <times/>
              <cn> 1 </cn>
              <apply>
                <divide/>
                <apply>
                  <power/>
                  <ci> half_maximal_inhibitory_concentration_nf_c3 </ci>
                  <cn> 2 </cn>
                </apply>
                <apply>
                  <plus/>
                  <apply>
                    <power/>
                    <ci> caspase_3 </ci>
                    <cn> 2 </cn>
                  </apply>
                  <apply>
                    <power/>
                    <ci> half_maximal_inhibitory_concentration_nf_c3 </ci>
                    <cn> 2 </cn>
                  </apply>
                </apply>
              </apply>
              <apply>
                <divide/>
                <apply>
                  <power/>
                  <ci> half_maximal_inhibitory_concentration_nf_inf </ci>
                  <cn> 2 </cn>
                </apply>
                <apply>
                  <plus/>
                  <apply>
                    <power/>
                    <ci> nf_kappaB_inhibitor </ci>
                    <cn> 2 </cn>
                  </apply>
                  <apply>
                    <power/>
                    <ci> half_maximal_inhibitory_concentration_nf_inf </ci>
                    <cn> 2 </cn>
                  </apply>
                </apply>
              </apply>
            </apply>
            <apply>
              <times/>
              <cn> 1 </cn>
              <ci> nf_kappaB </ci>
            </apply>
          </apply>
        </math>
      </rateRule>

      <!-- Nuclear transcription factor inhibitor -->
      <rateRule variable="nf_kappaB_inhibitor">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <minus/>
            <apply>
              <plus/>
              <apply>
                <times/>
                <cn> 0.5 </cn>
                <apply>
                  <divide/>
                  <apply>
                    <power/>
                    <ci> half_maximal_inhibitory_concentration_inf_tnf </ci>
                    <cn> 2 </cn>
                  </apply>
                  <apply>
                    <plus/>
                    <apply>
                      <power/>
                      <ci> tumour_necrosis_factor </ci>
                      <cn> 2 </cn>
                    </apply>
                    <apply>
                      <power/>
                      <ci> half_maximal_inhibitory_concentration_inf_tnf </ci>
                      <cn> 2 </cn>
                    </apply>
                  </apply>
                </apply>
              </apply>
              <apply>
                <times/>
                <cn> 0.5 </cn>
                <apply>
                  <divide/>
                  <apply>
                    <power/>
                    <ci> half_maximal_inhibitory_concentration_inf_nf </ci>
                    <cn> 2 </cn>
                  </apply>
                  <apply>
                    <plus/>
                    <apply>
                      <power/>
                      <ci> nf_kappaB </ci>
                      <cn> 2 </cn>
                    </apply>
                    <apply>
                      <power/>
                      <ci> half_maximal_inhibitory_concentration_inf_nf </ci>
                      <cn> 2 </cn>
                    </apply>
                  </apply>
                </apply>
              </apply>
            </apply>
            <apply>
              <times/>
              <cn> 1 </cn>
              <ci> nf_kappaB_inhibitor </ci>
            </apply>
          </apply>
        </math>
      </rateRule>
    </listOfRules>

    <listOfReactions>
      <!-- Synthesis of inactive receptor -->
      <reaction id="tnf_elimination" name="tnf_elimination" reversible="false" fast="false">
        <listOfReactants>
          <speciesReference species="tumour_necrosis_factor"/>
        </listOfReactants>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <ci> elimination_rate </ci>
              <ci> tumour_necrosis_factor </ci>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
    </listOfReactions>
  </model>
</sbml>
